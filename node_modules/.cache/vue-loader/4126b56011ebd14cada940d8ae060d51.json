 function refreshData(data) {\n      if (data) {\n        rankData.value = createRankData(data);\n      }\n    }\n    onMounted(() => {\n      initData();\n    });\n    return { rankData, curTitle,initData };\n  },\n});\n</script>\n\n<style lang=\"scss\" scoped>\n.transport-passenger-rank {\n  height: 100%;\n  .content {\n    height: calc(100% - 34px);\n    .chart {\n      position: relative;\n      height: 100%;\n      padding-left: 15px;\n      padding-top: 10px;\n    }\n  }\n}\n</style>\n"]}]}