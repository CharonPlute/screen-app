 function ():any {\n  const store = useStore();\n  //获取当前日期类型\n  const screenDataType = computed(() => {\n    return store.getters['app-global/GET_DATATYPE'];\n  });\n  //获取当前标题\n  const getTitle:any = (props) => {\n    return toRef(props, \"title\").value;\n  };\n  //初始化echarts的样式\n  const chartStyle = toRaw({\n    width: \"100%\",\n    \"min-height\": \"60px\",\n    height: \"100%\",\n  });\n  return {\n    screenDataType,\n    getTitle,\n    chartStyle,\n  };\n}\n"]}]}